{
  "name": "ai-kit",
  "version": "0.0.1",
  "scripts": {
    "dev": "blocklet dev",
    "eject": "vite eject",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development nodemon api/dev.ts -w api",
    "clean": "node scripts/build-clean.js",
    "bundle": "tsc --noEmit && npm run bundle:client && npm run bundle:api",
    "bundle:client": "vite build",
    "bundle:api": "npm run clean && tsc -p tsconfig.api.json && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "bump-version": "zx scripts/bump-version.mjs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcblock/did-auth": "^1.18.14",
    "@arcblock/did-auth-storage-nedb": "^1.7.1",
    "@blocklet/sdk": "^1.8.34",
    "@ocap/client": "^1.18.14",
    "@ocap/mcrypto": "^1.18.14",
    "@ocap/wallet": "^1.18.14",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vite-plugin-blocklet": "^0.5.6",
    "vite-plugin-svgr": "^2.2.2",
    "zx": "^7.1.1"
  }
}
